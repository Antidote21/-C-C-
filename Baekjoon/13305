#include <iostream>
#include <vector>
using namespace std;
int n;
int main(){

    cin>>n;

    vector<int> way(n-1, 0);
    vector<int> price(n, 0);

    
    for(int i=0; i<n-1; i++){
        cin>>way[i];
    }
    
    for(int i=0; i<n; i++){
        cin>>price[i];
    }
    
    int minPrice = way[0]*price[0];
    int v=1000;
    for(int i=1; i<n-1; i++){
        v = min(v, price[i]);
        minPrice += way[i]*v;
    }
    cout<<minPrice;
}
